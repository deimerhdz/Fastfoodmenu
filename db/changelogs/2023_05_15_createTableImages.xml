<?xml version="1.0" encoding="UTF-8"?>	
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns:pro="http://www.liquibase.org/xml/ns/pro"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">
     <changeSet id="2023051502" author="dhernandez">
        <createTable tableName="tbl_images">
            <column name="id" type="serial" remarks="Identificador autoincremental">
               <constraints primaryKey="true"/>
            </column>
            <column name="image_id" type="varchar(50)" remarks="Id de la imagen">
               <constraints nullable="false"/>
            </column>
              <column name="path_url" type="varchar(160)" remarks="Url de la imagen">
               <constraints nullable="false"/>
            </column>
        </createTable>
        <rollback>
          <dropTable tableName="tbl_images"/>  
        </rollback>
     </changeSet>
    
</databaseChangeLog>